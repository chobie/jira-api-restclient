{
  "name": "chobie/jira-api-restclient",
  "description": "JIRA REST API Client",
  "keywords": [
    "jira",
    "rest",
    "api"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Shuhei Tanuma",
      "homepage": "http://chobie.github.io/",
      "email": "chobieeee@php.net"
    }
  ],
  "require": {
    "php": ">=5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^5.0"
  },
  "autoload": {
    "psr-4": {
      "Chobie\\JiraApiRestClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Chobie\\Tests\\JiraApiRestClient\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
