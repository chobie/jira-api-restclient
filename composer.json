{
    "name": "chobie/jira-api-restclient",
    "description": "JIRA REST API Client",
    "keywords": ["jira","rest","api"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shuhei Tanuma",
            "homepage": "http://chobie.github.io/",
            "email": "chobieeee@php.net"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.1|^5.0|^6.0",
        "aik099/coding-standard": "dev-master",
        "squizlabs/php_codesniffer": "^2.6",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "autoload": {
        "psr-4" : {
            "chobie\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\chobie\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
